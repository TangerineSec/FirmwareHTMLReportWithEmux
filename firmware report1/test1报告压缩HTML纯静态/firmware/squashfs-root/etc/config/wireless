
config wifi-device 'ra0'
	option type 'ralink'
	option mode '9'
	option channel '0'
	option txpower '100'
	option ht '40'
	option hidessid '0'
	option macpolicy '0'
	option maclist ' '

config wifi-iface
	option device 'ra0'
	option ifname 'ra0'
	option network 'lan'
	option mode 'ap'
	option ssid 'xCloud_0001'
	option encryption 'none'

config wds-iface 'wds'
	option enable '0'
	option stat '0'
	option device 'apcli0'
	option ssid ''
	option bssid ''
	option channel '11'
	option authmode 'open'
	option encryptype 'none'
	option defaultkeyid '1'
	option key ''

config wifi-device 'rai0'
	option type 'mt7612'
	option ht '80'
	option channel '0'
	option hidessid '0'
	option macpolicy '0'
	option maclist ' '

config wifi-iface
	option device 'rai0'
	option ifname 'rai0'
	option network 'lan'
	option mode 'ap'
	option ssid 'xCloud_AC_0001'
	option encryption 'none'
